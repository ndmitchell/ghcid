{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": ["--extensionDevelopmentPath=${workspaceFolder}/plugins/vscode"],
            "sourceMaps": true,
            "outFiles": ["${workspaceFolder}/plugins/vscode/out/**/*.js"],
            "preLaunchTask": "npm: compile - plugins/vscode"
        }
    ]
}
